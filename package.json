{
  "name": "@chantouchsek/vue-photoswipe",
  "version": "0.0.1-beta-2",
  "description": "An image previewer for vue, powered by PhotoSwipe.",
  "main": "dist/index.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs && cp -p README.md docs",
    "build:lib": "vue-cli-service build --mode library --target lib --name VuePhotoSwipe --filename index --formats umd-min --dest dist ./lib/directive.js",
    "deploy": "npm run build:lib && npm publish --access=public",
    "lint": "vue-cli-service lint",
    "test": "echo test"
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Chantouch/vue-photoswipe.git"
  },
  "keywords": [
    "photoswipe",
    "preview",
    "vue"
  ],
  "author": {
    "name": "Chantouch Sek",
    "email": "chantouchsek.cs83@gmail.com",
    "url": "https://chantouchsek.github.io/"
  },
  "bugs": {
    "url": "https://github.com/Chantouch/vue-photoswipe/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "photoswipe": "^4.1.3",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
